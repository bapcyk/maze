        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Maze1"
        mc:Ignorable="d"
        Title="MainWindow" Height="480" Width="800">
    <Grid x:Name="Grid1">
        <Canvas x:Name="Canvas" HorizontalAlignment="Left" Height="384" Margin="0,27,0,0" VerticalAlignment="Top" Width="783"/>
        <ComboBox x:Name="Algorithms" HorizontalAlignment="Left" VerticalAlignment="Top" Width="783" SelectionChanged="Algorithms_SelectionChanged"/>

    </Grid>
</Window>
